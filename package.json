{
  "name": "zanejs_new",
  "private": true,
  "scripts": {
    "build": "cross-env NODE_OPTIONS=--max-old-space-size=8192 turbo build",
    "changeset": "pnpm exec changeset",
    "check": "pnpm run check:circular && pnpm run check:dep && pnpm run check:type && pnpm check:cspell",
    "check:circular": "node ./scripts/check-circular.mjs",
    "check:cspell": "cspell lint **/*.ts **/README.md .changeset/*.md --no-progress",
    "check:dep": "node ./scripts/check-dep.mjs",
    "check:type": "turbo run typecheck",
    "clean": "node ./scripts/clean.mjs",
    "commit": "czg",
    "dev": "turbo dev",
    "dev:docs": "pnpm -F @zanejs/docs run dev",
    "format": "node ./scripts/lint.mjs --format",
    "lint": "node ./scripts/lint.mjs",
    "check-types": "turbo run check-types",
    "postinstall": "pnpm -r run stub --if-present",
    "preinstall": "npx only-allow pnpm",
    "prepare": "is-ci || husky",
    "preview": "turbo-run preview",
    "reinstall": "pnpm clean --del-lock && pnpm install",
    "test:unit": "vitest run --dom",
    "test:e2e": "turbo run test:e2e",
    "update:deps": "npx taze -r -w"
  },
  "devDependencies": {
    "@changesets/changelog-github": "catalog:",
    "@changesets/cli": "catalog:",
    "@types/node": "catalog:",
    "@vite-pwa/vitepress": "catalog:",
    "@vitejs/plugin-vue": "catalog:",
    "@vitejs/plugin-vue-jsx": "catalog:",
    "@vueuse/core": "catalog:",
    "@zanejs/commitlint-config": "workspace:*",
    "@zanejs/eslint-config": "workspace:*",
    "@zanejs/node-utils": "workspace:*",
    "@zanejs/prettier-config": "workspace:*",
    "@zanejs/stylelint-config": "workspace:*",
    "@zanejs/tsconfig": "workspace:*",
    "@zanejs/vite-config": "workspace:*",
    "cross-env": "catalog:",
    "cspell": "catalog:",
    "eslint": "catalog:",
    "husky": "catalog:",
    "is-ci": "catalog:",
    "lint-staged": "catalog:",
    "prettier": "catalog:",
    "prettier-plugin-tailwindcss": "catalog:",
    "stylelint": "catalog:",
    "terser": "catalog:",
    "turbo": "catalog:",
    "typescript": "catalog:",
    "unbuild": "catalog:",
    "unocss": "catalog:",
    "unplugin-vue-components": "catalog:",
    "vite": "catalog:",
    "vitepress": "catalog:",
    "vitest": "catalog:"
  },
  "engines": {
    "node": ">=20.10.0",
    "pnpm": ">=10.12.4"
  },
  "packageManager": "pnpm@10.13.0"
}
